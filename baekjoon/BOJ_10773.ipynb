   "source": [
    "n = int(input())\n",
    "case = [int(input()) for _ in range(n)]\n",
    "my_stack = []\n",
    "\n",
    "def mistake(number):\n",
    "    if number == 0:\n",
    "        my_stack.pop()\n",
    "    else:\n",
    "        my_stack.append(number)\n",
    "\n",
    "for i in case:\n",
    "    mistake(i)\n",
    "\n",
    "print(sum(my_stack))"
   ],
