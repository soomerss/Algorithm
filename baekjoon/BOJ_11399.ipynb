   "source": [
    "n = int(input())\n",
    "case = list(map(int,input().split(' ')))\n",
    "\n",
    "def solution(case):\n",
    "    sum = 0\n",
    "    case.sort()\n",
    "    for i,v in enumerate(case):\n",
    "        sum += v * (len(case) - i)\n",
    "    return sum\n",
    "\n",
    "if __name__=='__main__':\n",
    "    answer = solution(case)\n",
    "    print(answer)"
