    "def sol(case):\n",
    "    profit = 0\n",
    "    min_price = sys.maxsize\n",
    "\n",
    "    for price in case:\n",
    "        min_price = min(price, min_price)\n",
    "        profit = max(profit,price - min_price)\n",
    "    return profit\n",
    "\n",
    "sol(case)"
